{
    "Novel.compile.separator": "*",
    "Novel.preview.lineLength": 23,
    "Novel.preview.linesPerPage": 31,
    "[applescript]": {
        "files.encoding": "macroman"
    },
    "[astro]": {
        "editor.defaultFormatter": "astro-build.astro-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [80],
        "editor.selectionHighlight": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[lua]": {
        "editor.defaultFormatter": "sumneko.lua"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "apple-swift-format.configSearchPaths": ["~/.swift-format"],
    "apple-swift-format.path": ["/opt/homebrew/bin//swift-format"],
    "atomKeymap.promptV3Features": true,
    "cSpell.userWords": ["Cica", "miharu"],
    "chat.editor.fontSize": 13,
    "cmake.configureOnOpen": false,
    "cmake.showOptionsMovedNotification": false,
    "code-runner.cwd": "$dirWithoutTraillingSlash",
    "code-runner.executorMap": {
        "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "ahk": "autohotkey",
        "applescript": "osascript",
        "autoit": "autoit3",
        "bat": "cmd /c",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "clojure": "lein exec",
        "coffeescript": "coffee",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "crystal": "crystal",
        "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "dart": "dart",
        "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "fsharp": "fsi",
        "go": "go run",
        "groovy": "groovy",
        "haskell": "runghc",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $dir$fileNameWithoutExt",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "javascript": "node",
        "julia": "julia",
        "kit": "kitc --run",
        "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
        "lisp": "sbcl --script",
        "lua": "lua",
        "mojo": "mojo run",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "ocaml": "ocaml",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "perl": "perl",
        "perl6": "perl6",
        "php": "php",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "python3": "python3 -u",
        "r": "Rscript",
        "racket": "racket",
        "ruby": "ruby",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "sass": "sass --style expanded",
        "scala": "scala",
        "scheme": "csi -script",
        "scss": "scss --style expanded",
        "shellscript": "bash",
        "sml": "cd $dir && sml $fileName",
        "swift": "swift",
        "typescript": "ts-node",
        "v": "v run",
        "vbscript": "cscript //Nologo",
        "zig": "zig run"
    },
    "code-runner.ignoreSelection": true,
    "code-runner.runInTerminal": true,
    "csharp.debug.console": "externalTerminal",
    "csharp.debug.hotReloadVerbosity": "diagnostic",
    "csharp.inlayHints.enableInlayHintsForImplicitObjectCreation": true,
    "csharp.inlayHints.enableInlayHintsForImplicitVariableTypes": true,
    "css.format.spaceAroundSelectorSeparator": true,
    "css.lint.float": "warning",
    "css.lint.important": "warning",
    "css.styleSheets": [],
    "cssPeek.trace.server": "messages",
    "csv-edit.fontSizeInPx": 13,
    "debug.allowBreakpointsEverywhere": true,
    "debug.terminal.clearBeforeReusing": true,
    "diffEditor.codeLens": true,
    "dotnet.backgroundAnalysis.analyzerDiagnosticsScope": "fullSolution",
    "dotnet.backgroundAnalysis.compilerDiagnosticsScope": "fullSolution",
    "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchArgumentName": true,
    "dotnet.inlayHints.suppressInlayHintsForParametersThatMatchMethodIntent": true,
    "dotnet.unitTestDebuggingOptions": {},
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.autoClosingComments": "beforeWhitespace",
    "editor.autoClosingDelete": "never",
    "editor.autoIndent": "advanced",
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "line-thin",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.definitionLinkOpensInPeek": true,
    "editor.find.globalFindClipboard": true,
    "editor.find.seedSearchStringFromSelection": "selection",
    "editor.fontFamily": "Moralerspace Xenon NF",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "editor.formatOnType": true,
    "editor.inlayHints.fontFamily": "Moralerspace Neon NF",
    "editor.multiCursorMergeOverlapping": false,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "all",
    "editor.stickyScroll.enabled": true,
    "editor.suggest.insertMode": "replace",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "extensions.ignoreRecommendations": true,
    "files.autoGuessEncoding": true,
    "files.autoSave": "onFocusChange",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.openRepositoryInParentFolders": "never",
    "github.copilot.advanced": {},
    "github.copilot.editor.enableAutoCompletions": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "csharp": false
    },
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true
    },
    "hediet.vscode-drawio.local-storage": "",
    "hexeditor.columnWidth": 16,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "hexeditor.showDecodedText": true,
    "html.format.wrapLineLength": 60,
    "indentRainbow.colorOnWhiteSpaceOnly": true,
    "intellicode-completions-cdk.runAlongsideCopilot": true,
    "java.selectionRange.enabled": false,
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindow.creationMode": "perFile",
    "keyboard.touchbar.enabled": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "lldb.launch.expressions": "native",
    "lldb.library": "/Applications/Xcode-16.1.0.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
    "markdown-preview-enhanced.codeBlockTheme": "monokai.css",
    "markdownlint.config": {
        "MD007": false
    },
    "material-icon-theme.activeIconPack": "react",
    "mojo.modularHomePath": "~/.modular/",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "notebook.cellToolbarVisibility": "hover",
    "notebook.formatOnSave.enabled": true,
    "notebook.lineNumbers": "on",
    "notebook.output.scrolling": true,
    "notebook.output.textLineLimit": 15,
    "notebook.output.wordWrap": true,
    "oneDarkPro.vivid": false,
    "prettier.tabWidth": 4,
    "python.analysis.cacheFolderPath": "~/.pyenv/shims/python",
    "python.analysis.extraPaths": ["~/.pyenv/shims/python"],
    "python.analysis.typeshedPaths": ["~/.pyenv/shims/python"],
    "python.autoComplete.extraPaths": ["~/.pyenv/shims/python"],
    "python.pythonPath": "/usr/local/bin/python3",
    "razor.trace": "Messages",
    "redhat.telemetry.enabled": false,
    "rust-analyzer.assist.emitMustUse": true,
    "rust-analyzer.check.command": "clippy",
    "screencastMode.fontSize": 24,
    "security.workspace.trust.untrustedFiles": "open",
    "settingsSync.ignoredSettings": ["-swift.sanitizer"],
    "swift.diagnostics": true,
    "swift.sanitizer": "thread",
    "swift.sourcekit-lsp.serverPath": "/usr/local/bin/sourcekit-lsp",
    "swift.sourcekit-lsp.trace.server": "verbose",
    "swiftformat.configSearchPaths": ["~/.swiftformat"],
    "tabnine.experimentalAutoImports": true,
    "terminal.external.osxExec": "",
    "terminal.external.windowsExec": "",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.enableImages": true,
    "terminal.integrated.env.osx": {
        "CW_NEW_SESSION": 1,
        "FIG_NEW_SESSION": 1,
        "Q_NEW_SESSION": "1"
    },
    "terminal.integrated.fontFamily": "Moralerspace Neon HWNF",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.macOptionClickForcesSelection": true,
    "terminal.integrated.minimumContrastRatio": 1,
    "update.showReleaseNotes": false,
    "vim.autoSwitchInputMethod.defaultIM": "com.kawasemi4.keylayout.US",
    "vim.cursorStylePerMode.insert": "line-thin",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "line",
    "vim.cursorStylePerMode.visual": "block-outline",
    "vim.cursorStylePerMode.visualblock": "block-outline",
    "vim.cursorStylePerMode.visualline": "underline",
    "vim.easymotion": true,
    "vs-kubernetes": {
        "vs-kubernetes.draft-path.mac": "~/.vs-kubernetes/tools/draft/darwin-amd64/draft",
        "vs-kubernetes.helm-path.mac": "~/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.kubectl-path.mac": "~/.vs-kubernetes/tools/kubectl/kubectl",
        "vs-kubernetes.minikube-path.mac": "~/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vscode-kubernetes.helm-path.mac": "~/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vscode-kubernetes.kubectl-path.mac": "~/.vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.minikube-path.mac": "~/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "vscode-office.editorTheme": "Light",
    "vscode-office.openOutline": true,
    "vscode-w3cvalidation.validator-token": "Fri, 25 Jun 2021 22:00:00 GMT",
    "window.autoDetectColorScheme": true,
    "window.autoDetectHighContrast": false,
    "workbench.activityBar.iconClickBehavior": "focus",
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.editorAssociations": {
        "*.db": "default",
        "*.dic": "default",
        "*.ipynb": "jupyter-notebook",
        "*.md": "default",
        "*.pdf": "default",
        "{git,gitlens}:/**/*.{md,csv,svg}": "default"
    },
    "workbench.iconTheme": "a-file-icon-vscode",
    "workbench.preferredDarkColorTheme": "Catppuccin Macchiato",
    "workbench.preferredLightColorTheme": "Catppuccin Mocha",
    "workbench.settings.applyToAllProfiles": ["workbench.colorTheme"],
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.indent": 20,
    "zig.initialSetupDone": true,
    "zig.path": "zig",
    "zig.zls.path": "zls",
    "workbench.productIconTheme": "a-file-icon-vscode-product-icon-theme"
}
