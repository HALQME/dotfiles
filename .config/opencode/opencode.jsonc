{
  "$schema": "https://opencode.ai/config.json",
  "autoupdate": true,
  // Theme configuration
  "theme": "opencode",
  "agent": {},
  "instructions": [
    ".*/**/CONTRIBUTING.md",
    ".*/**/*.md"
  ],
  "model": "lmstudio/qwen/qwen3-coder-30b",
  "provider": {
    "lmstudio": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "LM Studio",
      "options": {
        "baseURL": "http://127.0.0.1:1234/v1"
      },
      "models": {
        "qwen/qwen3-coder-30b": {
          "name": "Qwen Coder 30B",
          "tool_call": true,
          "limit": {
            "context": 16384,
            "output": 8192
          },
          "cost": {
            "cache_read": 0,
            "cache_write": 0,
            "input": 0,
            "output": 0
          },
          "id": "lmstudio/qwen/qwen3-coder-30b",
          "reasoning": false,
          "temperature": true,
          "attachment": true,
          "release_date": "2025-08-02",
          "options": {}
        }
      }
    }
  },
  "mcp": {
    "serena": {
      "type": "local",
      "enabled": true,
      "command": [
        "/opt/homebrew/bin/uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server"
      ],
      "environment": {}
    }
  },
  "lsp": {}
}
