{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Fireworks",
  "author": "HALQME <halqme@icloud.com>",
  "themes": [
    {
      "name": "Fireworks Dark",
      "appearance": "dark",
      "style": {
        "accents": [
          "#c8b1ff",
          "#8ab4f8",
          "#82e9ff",
          "#a8e67e",
          "#f0c674",
          "#ffb870",
          "#ff879f"
        ],
        // --- UI ---
        // --- 背景 & ボーダー ---
        "background.appearance": "blurred",
        "background": "#101014ca",
        "border": "#3b426190",
        "border.variant": "#4f587e99",
        "border.focused": "#8ab4f8",
        "border.selected": "#c8b1ff",
        "border.transparent": "#a8e67e",
        "border.disabled": "#565f89",
        "elevated_surface.background": "#24283b",
        "surface.background": "#1a1d2bda",
        // --- UI要素 ---
        "element.background": "#24283b",
        "element.hover": "#292e42",
        "element.active": "#00000000",
        "element.selected": "#292e4220",
        "element.disabled": "#565f89",
        "drop_target.background": "#8ab4f820",
        "ghost_element.background": "#00000000",
        "ghost_element.hover": "#8ab4f810",
        "ghost_element.active": "#8ab4f840",
        "ghost_element.selected": "#8ab4f840",
        "ghost_element.disabled": "#565f89",
        // --- テキスト & アイコン ---
        "text": "#c0caf5",
        "text.muted": "#a9b1d6",
        "text.placeholder": "#4f587e",
        "text.disabled": "#3b4261",
        "text.accent": "#c8b1ff",
        "icon": "#c0caf5",
        "icon.muted": "#848cb5",
        "icon.disabled": "#565f89",
        "icon.placeholder": "#4f587e",
        "icon.accent": "#c8b1ff",
        // --- バー & パネル ---
        "status_bar.background": "#16161ed0",
        "title_bar.background": "#16161ed0",
        "title_bar.inactive_background": "#121216d0",
        "tab_bar.background": "#10202015",
        "toolbar.background": "#10202030",
        "tab.inactive_background": "#00000005",
        "tab.active_background": "#3b4261",
        "search.match_background": "#82e9ff44",
        "panel.background": "#6A5D4501",
        "panel.focused_border": "00000000",
        "panel.indent_guide": "#32385499",
        "panel.indent_guide_active": "#4f587e",
        "panel.indent_guide_hover": "#c8b1ff",
        "pane.focused_border": "#c0caf5",
        "pane_group.border": "#292e42",
        // --- スクロールバー ---
        "scrollbar.thumb.background": "#8ab4f815",
        "scrollbar.thumb.hover_background": "#565f8980",
        "scrollbar.thumb.border": "#c8b1ff",
        "scrollbar.track.background": "#00000000",
        "scrollbar.track.border": "#00000000",
        // --- エディタ ---
        "editor.foreground": "#c0caf5",
        "editor.background": "#10202015",
        "editor.gutter.background": "#10202015",
        "editor.subheader.background": "#16161e",
        "editor.active_line.background": "#8ab4f80A",
        "editor.highlighted_line.background": "#8ab4f815",
        "editor.line_number": "#7F9CE590",
        "editor.active_line_number": "#8ab4f8",
        "editor.invisible": "#848cb533",
        "editor.wrap_guide": "#4f587e",
        "editor.active_wrap_guide": "#4f587e",
        "editor.document_highlight.bracket_background": "#c8b1ff17",
        "editor.document_highlight.read_background": "#a9b1d629",
        "editor.document_highlight.write_background": "#a9b1d629",
        "editor.indent_guide": "#32385499",
        "editor.indent_guide_active": "#4f587e",
        "link_text.hover": "#82e9ff",
        // --- ターミナル ---
        "terminal.background": "#1A1D2505",
        "terminal.ansi.background": "#1f2335CC",
        "terminal.foreground": "#a9b1d6",
        "terminal.dim_foreground": "#848cb5",
        "terminal.bright_foreground": "#c0caf5",
        // --- ANSIカラー ---
        "terminal.ansi.black": "#353b51",
        "terminal.ansi.red": "#e8667e",
        "terminal.ansi.green": "#8fbc5d",
        "terminal.ansi.yellow": "#d09f58",
        "terminal.ansi.blue": "#6a92e7",
        "terminal.ansi.magenta": "#aa89e7",
        "terminal.ansi.cyan": "#6dc0ef",
        "terminal.ansi.white": "#8f9aaf",
        "terminal.ansi.bright_black": "#4f587e",
        "terminal.ansi.bright_red": "#ff879f",
        "terminal.ansi.bright_green": "#a8e67e",
        "terminal.ansi.bright_yellow": "#f0c674",
        "terminal.ansi.bright_blue": "#8ab4f8",
        "terminal.ansi.bright_magenta": "#c8b1ff",
        "terminal.ansi.bright_cyan": "#82e9ff",
        "terminal.ansi.bright_white": "#c0caf5",
        "terminal.ansi.dim_black": "#353b51",
        "terminal.ansi.dim_red": "#e8667e",
        "terminal.ansi.dim_green": "#8fbc5d",
        "terminal.ansi.dim_yellow": "#d09f58",
        "terminal.ansi.dim_blue": "#6a92e7",
        "terminal.ansi.dim_magenta": "#aa89e7",
        "terminal.ansi.dim_cyan": "#6dc0ef",
        "terminal.ansi.dim_white": "#8f9aaf",
        // --- バージョン管理 ---
        "conflict": "#ffb870",
        "conflict.border": "#ffb870",
        "conflict.background": "#ffb87026",
        "created": "#a8e67e",
        "created.border": "#a8e67e",
        "created.background": "#a8e67e26",
        "deleted": "#ff879f",
        "deleted.border": "#ff879f",
        "deleted.background": "#ff879f26",
        "hidden": "#565f89",
        "hidden.border": "#565f89",
        "hidden.background": "#16161e",
        "hint": "#848cb5",
        "hint.border": "#4f587e",
        "hint.background": "#292e42c0",
        "ignored": "#565f89",
        "ignored.border": "#565f89",
        "ignored.background": "#565f8926",
        "modified": "#f0c674",
        "modified.border": "#f0c674",
        "modified.background": "#f0c67426",
        "predictive": "#565f89",
        "predictive.border": "#8ab4f8",
        "predictive.background": "#16161e",
        "renamed": "#82e9ff",
        "renamed.border": "#82e9ff",
        "renamed.background": "#82e9ff26",
        "info": "#82e9ff",
        "info.border": "#82e9ff",
        "info.background": "#292e42",
        "warning": "#f0c674",
        "warning.border": "#f0c674",
        "warning.background": "#292e42",
        "error": "#ff879f",
        "error.border": "#ff879f",
        "error.background": "#292e42",
        "success": "#a8e67e",
        "success.border": "#a8e67e",
        "success.background": "#292e42",
        "unreachable": "#ff879f",
        "unreachable.border": "#ff879f",
        "unreachable.background": "#ff879f1f",
        // --- シンタックスハイライト ---
        "syntax": {
          "variable": {
            "color": "#c0caf5"
          },
          "variable.builtin": {
            "color": "#ff879f"
          },
          "variable.parameter": {
            "color": "#ffb870"
          },
          "variable.member": {
            "color": "#c0caf5"
          },
          "variable.special": {
            "color": "#ff879f",
            "font_style": "italic"
          },
          "constant": {
            "color": "#ffb870"
          },
          "constant.builtin": {
            "color": "#ffb870"
          },
          "constant.macro": {
            "color": "#c8b1ff"
          },
          "module": {
            "color": "#f0c674",
            "font_style": "italic"
          },
          "label": {
            "color": "#8ab4f8"
          },
          "string": {
            "color": "#a8e67e"
          },
          "string.documentation": {
            "color": "#6b759d"
          },
          "string.regexp": {
            "color": "#f0c674"
          },
          "string.escape": {
            "color": "#c8b1ff"
          },
          "string.special": {
            "color": "#c8b1ff"
          },
          "string.special.symbol": {
            "color": "#dd7973"
          },
          "string.special.url": {
            "color": "#ea9a97",
            "font_style": "italic"
          },
          "character": {
            "color": "#a8e67e"
          },
          "character.special": {
            "color": "#c8b1ff"
          },
          "boolean": {
            "color": "#ffb870"
          },
          "number": {
            "color": "#ffb870"
          },
          "type": {
            "color": "#f0c674"
          },
          "type.builtin": {
            "color": "#82e9ff",
            "font_style": "italic"
          },
          "type.definition": {
            "color": "#f0c674"
          },
          "type.interface": {
            "color": "#f0c674",
            "font_style": "italic"
          },
          "type.super": {
            "color": "#f0c674",
            "font_style": "italic"
          },
          "attribute": {
            "color": "#f0c674"
          },
          "property": {
            "color": "#c0caf5"
          },
          "function": {
            "color": "#8ab4f8"
          },
          "function.builtin": {
            "color": "#ffb870"
          },
          "function.call": {
            "color": "#8ab4f8"
          },
          "function.macro": {
            "color": "#82e9ff"
          },
          "function.method": {
            "color": "#8ab4f8"
          },
          "function.method.call": {
            "color": "#8ab4f8"
          },
          "constructor": {
            "color": "#dd7973"
          },
          "operator": {
            "color": "#89ddff"
          },
          "keyword": {
            "color": "#c8b1ff"
          },
          "punctuation": {
            "color": "#565f89"
          },
          "comment": {
            "color": "#6b759d",
            "font_style": "italic"
          },
          "diff.plus": {
            "color": "#a8e67e"
          },
          "diff.minus": {
            "color": "#ff879f"
          },
          "tag": {
            "color": "#ff879f"
          },
          "tag.attribute": {
            "color": "#f0c674",
            "font_style": "italic"
          },
          "tag.delimiter": {
            "color": "#82e9ff"
          },
          "emphasis.strong": {
            "color": "#e06b7e",
            "font_weight": 700
          },
          "emphasis": {
            "color": "#e06b7e",
            "font_style": "italic"
          },
          "link_text": {
            "color": "#a9b1d6"
          },
          "link_uri": {
            "color": "#8ab4f8",
            "font_style": "italic"
          },
          "title": {
            "color": "#c0caf5",
            "font_weight": 800
          }
        },
        // --- プレイヤー ---
        "players": [
          {
            "cursor": "#ea9a97",
            "selection": "#4f587e80",
            "background": "#ea9a97"
          },
          {
            "cursor": "#c8b1ff",
            "selection": "#c8b1ff33",
            "background": "#c8b1ff"
          },
          {
            "cursor": "#8ab4f8",
            "selection": "#8ab4f833",
            "background": "#8ab4f8"
          },
          {
            "cursor": "#82e9ff",
            "selection": "#82e9ff33",
            "background": "#82e9ff"
          },
          {
            "cursor": "#a8e67e",
            "selection": "#a8e67e33",
            "background": "#a8e67e"
          },
          {
            "cursor": "#f0c674",
            "selection": "#f0c67433",
            "background": "#f0c674"
          },
          {
            "cursor": "#ffb870",
            "selection": "#ffb87033",
            "background": "#ffb870"
          },
          {
            "cursor": "#ff879f",
            "selection": "#ff879f33",
            "background": "#ff879f"
          }
        ]
      }
    }
  ]
}
