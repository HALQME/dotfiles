[
  {
    "label": "lazygit",
    "command": "lazygit",
    "reveal_target": "center",
    "hide": "always"
  },
  {
    "label": "tmux",
    "command": "tmux attach || tmux new",
    "allow_concurrent_runs": true,
    "reveal_target": "center",
    "hide": "always"
  },
  {
    "label": "act",
    "command": "act --container-architecture linux/amd64 -s GITHUB_TOKEN=\"$(gh auth token)\"",
    "reveal_target": "center",
    "hide": "always"
  },
  {
    "label": "btm",
    "command": "btm",
    "reveal_target": "center",
    "hide": "always"
  },
  {
    "label": "zsh",
    "command": "zsh",
    "use_new_terminal": true,
    "allow_concurrent_runs": true,
    "reveal_target": "center"
  }
]
